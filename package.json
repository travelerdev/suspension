{
  "name": "suspension",
  "version": "0.1.0",
  "description": "React hooks and utilities for using Suspense with existing APIs",
  "main": "dist/index.js",
  "module": "dist/suspension.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zackdotcomputer/suspension.git"
  },
  "keywords": [
    "react",
    "suspense",
    "concurrent",
    "experimental",
    "frontend",
    "hook",
    "typescript"
  ],
  "author": "Zack Sheppard <hi@zack.computer> (https://www.zack.computer)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zackdotcomputer/suspension/issues"
  },
  "homepage": "https://github.com/zackdotcomputer/suspension#readme",
  "peerDependencies": {
    "react": ">=16.13.0"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/suspension.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/suspension.esm.js",
      "limit": "10 KB"
    }
  ],
  "files": [
    "/dist",
    "/src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.0",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "husky": "^5.1.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.10.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "is-promise": "^4.0.0",
    "react-error-boundary": "^3.1.1"
  }
}
